        <Image Canvas.Left="64" Canvas.Top="318" Height="170" Name="image1" Stretch="Fill" Width="198" Source="/HerculesWPFGameMenu;component/Images/memory-icon.png" MouseUp="image1_MouseUp" />
        <Image Canvas.Left="303" Canvas.Top="318" Height="170" Name="image3" Stretch="Fill" Width="200" Source="/HerculesWPFGameMenu;component/Images/draw-icon.png" MouseUp="image3_MouseUp" />
