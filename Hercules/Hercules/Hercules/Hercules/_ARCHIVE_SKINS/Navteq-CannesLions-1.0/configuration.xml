<Hercules configver="1">


 <!-- MountainDewDemo Skin -->

  <!-- How many photos are printed (4, 6) -->
  <add key="PHOTO_COUNT" value="6" />

  <!-- How many prints (1=single print, 2=two prints .. up to 99999 -->
